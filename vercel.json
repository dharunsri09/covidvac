{
    "builds": [{
        "src": "covidVac/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "env": {
        "PY_DEP": "sqlite3"
      },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "covidVac/wsgi.py"
        }
    ]
}
